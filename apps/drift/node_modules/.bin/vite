#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules/vite/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules/vite/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules/vite/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules/vite/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/vite@7.2.4_@types+node@25.2.0_jiti@2.4.2_terser@5.46.0/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
