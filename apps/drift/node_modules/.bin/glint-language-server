#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/ember-tsc/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/ember-tsc/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/ember-tsc/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/ember-tsc/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules/@glint/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/@glint+ember-tsc@1.0.8_typescript@5.9.3/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@glint/ember-tsc/bin/glint-language-server.js" "$@"
else
  exec node  "$basedir/../@glint/ember-tsc/bin/glint-language-server.js" "$@"
fi
