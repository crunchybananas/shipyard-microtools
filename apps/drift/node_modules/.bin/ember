#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules/ember-cli/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules/ember-cli/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules/ember-cli/bin/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules/ember-cli/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/ember-cli@6.9.1_@types+node@25.2.0_handlebars@4.7.8_underscore@1.13.7/node_modules:/Users/cloken/code/Dockhand/shipyard-microtools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ember-cli/bin/ember" "$@"
else
  exec node  "$basedir/../ember-cli/bin/ember" "$@"
fi
